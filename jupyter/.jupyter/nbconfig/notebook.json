{
  "load_extensions": {
    "nbextensions_configurator/config_menu/main": true,
    "nbextensions_configurator": true,
    "collapsible_headings/main": true,
    "hide_input/main": true,
    "toc2/main": true,
    "spellchecker/main": true
  },
  "rise": {
    "enable_chalkboard": false,
    "show_buttons_on_startup": true
  },
  "Notebook": {
    "Header": false,
    "Toolbar": false
  },
  "toc2": {
    "number_sections": false
  },
  "Cell": {
    "cm_config": {
      "lineNumbers": false
    }
  },
  "keys": {
    "command": {
      "bind": {
        "q,a": "jupyter-notebook:run-all-cells-above",
        "cmd-left": "jupyter-notebook:run-all-cells-above",
        "q,b": "jupyter-notebook:run-all-cells-below",
        "cmd-right": "jupyter-notebook:run-all-cells-below",
        "q,q": "jupyter-notebook:run-all-cells",
        "cmd-enter": "jupyter-notebook:run-all-cells",
        "cmd-shift-enter": "jupyter-notebook:restart-kernel-and-run-all-cells",
        "q,x": "jupyter-notebook:close-and-halt",
        "t,i": "hide_input:toggle-cell-input-display",
        "alt-shift-i": "hide_input:toggle-cell-input-display",
        "t,m": "jupyter-notebook:toggle-menubar",
        "t,t": "toc2:toggle-toc",
        "f2": "jupyter-notebook:rename-notebook",
        "q,r": "jupyter-notebook:rename-notebook"
      },
      "unbind": [
        "q"
      ]
    }
  },
  "CodeCell": {
    "cm_config": {
      "autoCloseBrackets": false
    }
  },
  "collapsible_headings": {
    "use_shortcuts": true,
    "shortcuts": {
      "collapse": "h",
      "uncollapse": "l",
      "collapse_all": "shift-h",
      "uncollapse_all": "shift-l"
    }
  }
}